# 1
def solution(numbers):
    answer = [ i*2 for i in numbers]
    return answer
    
# 2
n= input().split()
print(len(n))
for i in n:
  if (len(n)) % 2 == 1:
    print(int(i) * 2, end=" " )
  else:
    print(int(i) // 2, end=" ")
    
# 3
def solution(n):
    return sum([i for i in range(2, n+1, 2)])
