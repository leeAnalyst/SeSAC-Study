# 숫자찾기
def solution(num, k):
  if str(k) in str(num) :
    return (str(num).find(str(k)))+1
  else:
    return -1
    
    
# 문자열 계산하기
def solution(my_string):
  return eval(my_string)
  
  
# 가장 큰 수 찾기
def solution(array):
  return [max(array), array.index(max(array))] 
  
  
# 편지
def solution(message):
  return (len(list(message)))*2
  

# 약수구하기
def solution(n):
  answer = []
  for i in range(1, n+1):
    if n % i == 0:
      answer.append(i)
  return answer
