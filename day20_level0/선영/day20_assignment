# 직사각형 넓이 구하기 
def solution(dots):
  return (max(dots)[0] - min(dots)[0]) * (max(dots)[1] - min(dots)[1])
  

# 배열 원소의 길이
def solution(strlist):
  return [len(strlist[i]) for i in range (len(strlist))]


# 숨어있는 숫자의 덧셈 (1)
import re

def solution(my_string):
  result = re.findall(r'\d', my_string)
  answer = 0
  for i in result:
    answer += int(i)
  return answer
