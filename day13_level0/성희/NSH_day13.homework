# 잘라서 배열하기

my_str = input()
n = int(input())
words = len(my_str)
s = words // n
r = words % n

answer = []
if r != 0:
    for i in range(s):
        answer.append(my_str[i*n : (i+1)*n])
    answer.append(my_str[s*n :])
else:
    for i in range(s):
        answer.append(my_str[i*n : (i+1)*n])
print(answer)


