# 연속된 수의 합
num, total = 4, 14
share = total // num
remainder = total % num
print(f'share:{share}, remainder:{remainder}')

count = 0
answer = []
if remainder == 0 :
    answer.append(share)
    while len(answer) < num:
        count += 1
        answer.append(share - count)
        print(share-count)
        answer.append(share + count)
else:
    answer.append(share)
    answer.append(share + 1)
    while len(answer) < num:
        count += 1
        answer.append(share - count)
        answer.append(share+1 + count)
answer = sorted(answer)
print(answer)

