# 외계어 사전 
def solution(spell, dic):
  spell = set(spell)   # 한번씩만 사용, 중복된 원소를 갖지 않음 set
  for i in dic:
    if spell.issubset(set(i)):  # dic에 있는 중복되지 않는 요소가 배열 spell의 하위집합이면 1 아니면 2
      return 1
  return 2
  

# 삼각형의 완성조건(2)
def solution(sides):
  return (sorted(sides)[0] * 2) -1
  

# 숨어있는 숫자의 덧셈
import re

def solution(my_string):
  result = re.findall(r'\d+', my_string)
  answer = 0
  for i in result:
    answer += int(i)
  return (answer)
  


# 최댓값 만들기(2)
def solution(numbers):
    numbers.sort(reverse = True)
    return max(numbers[0] * numbers[1],numbers[-1] * numbers[-2])
